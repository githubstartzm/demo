<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ali.superbaby.dao.UserDao">
    <select id="login" parameterType="com.ali.superbaby.form.LoginForm" resultType="com.ali.superbaby.entity.LoginEntity">
        select * from mb_user where username = #{username} and password = #{password}
    </select>
</mapper>